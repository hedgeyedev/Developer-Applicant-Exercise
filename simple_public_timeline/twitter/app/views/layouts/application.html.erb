<!DOCTYPE html>
<html>
  <head>
    <title>Twitter</title>
    <%= csrf_meta_tags %>
    <!-- jquery -->
<!--     <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script> -->
    <!-- linkify to render automatically href as link in the tweet content -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<!--     <script type="text/javascript" src="linkify-string.js.js"></script>
    <script type="text/javascript" src="application.js"></script> -->
    
    <!-- internal stylesheet -->
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
   <!--  internal javascript scripts -->
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- external css librairies -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    
  </head>

  <body>
  
  <div class="container">
    <div class="row">
      <div class="col-12 white">
          <div class="white-main">
          <h1 class="top-line">Recent Public Tweets</h1>
          <h3>What everyone on Twitter is talking about!</h3>

    <%= yield %>

          </div>
      </div>
    </div>
</div>


  </body>
</html>
